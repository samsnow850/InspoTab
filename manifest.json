{
  "manifest_version": 3,
  "name": "InspoTab",
  "description": "Replaces your new tab with a daily motivational quote and beautiful background.",
  "version": "2.0",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "images/logo.png"
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*"
  ]
} 